{
  "enabled": 1,
  "hidden": false,
  "description": "Jobset to declaratively generate other jobsets",
  "nixexprinput": "kontrakcja",
  "nixexprpath": "nix/hydra/default.nix",
  "checkinterval": 30,
  "schedulingshares": 100,
  "enableemail": false,
  "emailoverride": "",
  "keepnr": 3,
  "inputs": {
    "kontrakcja": {
      "type": "git",
      "value": "git@github.com:scrive/kontrakcja.git",
      "emailresponsible": false
    },
    "nixpkgs": {
      "type": "git",
      "value": "git://github.com/NixOS/nixpkgs.git release-18.09",
      "emailresponsible": false
    }
  }
}

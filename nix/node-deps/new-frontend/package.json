{
  "name": "new_frontend-loggedin",
  "private": true,
  "version": "0.0.0",
  "description": "",
  "config": {
    "BACKEND_API": "https://dev.scrive.com"
  },
  "scripts": {
    "prod-build": "rm -rf dist && PUBLIC_PATH=/new webpack --config webpack/prod.js",
    "prod-build-portal": "rm -rf dist && PUBLIC_PATH=/portal webpack --config webpack/prod.js",
    "start": "webpack-dev-server --config webpack/serve.js --host 0.0.0.0",
    "check-prod-build": "NODE_ENV=development npm run prod-build && SERVING_PATH=/new node prodTest/server.js",
    "check-prod-build-portal": "NODE_ENV=development npm run prod-build-portal && SERVING_PATH=/portal node prodTest/server.js",
    "test": "jest --runInBand",
    "test:watch": "npm test -- --watch",
    "precommit": "lint-staged && npm run test",
    "pretty": "prettier --end-of-line lf --write",
    "get-translations": "cd transifex && node getTranslations.js",
    "push-resource": "cd transifex && node pushResource.js",
    "unused-translations": "cd transifex/unused-keys-scripts && node unusedTranslations.js",
    "unremoved-in-current-branch": "cd transifex/unused-keys-scripts && node unremovedInCurrentBranch.js",
    "precommit-unremoved-trans-keys": "cd transifex/unused-keys-scripts && node precommitUnremovedTransKeys.js",
    "test:e2e:chrome": "cp tsconfig/chrome-testcaferc.json .testcaferc.json && testcafe",
    "test:e2e:chrome:headless": "cp tsconfig/chrome-headless-testcaferc.json .testcaferc.json && testcafe",
    "test:e2e:firefox": "cp tsconfig/firefox-testcaferc.json .testcaferc.json && testcafe",
    "test:e2e:firefox:headless": "cp tsconfig/firefox-headless-testcaferc.json .testcaferc.json && testcafe",
    "docker:start": "docker-compose run --rm start",
    "docker:e2e:chrome": "cp tsconfig/docker-testcaferc.json .testcaferc.json && docker-compose run --rm e2e-test-chrome",
    "docker:e2e:firefox": "cp tsconfig/docker-testcaferc.json .testcaferc.json && docker-compose run --rm e2e-test-firefox"
  },
  "lint-staged": {
    "!(tests/)**/*.{js,jsx}": [
      "npm run pretty",
      "git add"
    ],
    "tests/**/*.js": [
      "npm run pretty"
    ],
    "*.{json,less,css}": [
      "npm run pretty",
      "git add"
    ]
  },
  "jest": {
    "modulePaths": [
      "<rootDir>"
    ],
    "transform": {
      "^.+\\.(js|jsx)?$": "babel-jest"
    },
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "author": "Linus Ek & Friends",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",

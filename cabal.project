packages: . ./Shake

write-ghc-environment-files: never

repository hackage.scrive.com
    url: https://hackage.scrive.com/

package *
    ghc-options: -j4 +RTS -A32m -RTS

package text-icu
    extra-include-dirs: /usr/local/opt/icu4c/include
    extra-lib-dirs: /usr/local/opt/icu4c/lib

source-repository-package
    type: git
    location: https://github.com/dylex/hsaml2.git
    tag: d4852ee7d96f7643c97e5a7f1a82a7705a0e1236

allow-newer: servant-docs:lens
             servant:QuickCheck
